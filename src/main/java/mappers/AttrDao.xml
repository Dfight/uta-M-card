<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="uta.macross.dao.AttrDao">
    <insert id="addAttr" parameterType="uta.macross.entry.attribute">
        insert into attribute(Attr_Name,Attr_IMG) VALUES(#{Attr_Name},#{Attr_IMG})
    </insert>
    <select id="queryAllAttr" resultType="uta.macross.entry.attribute">
        Select * from attribute
    </select>



</mapper>